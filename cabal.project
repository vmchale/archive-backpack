packages: hstar
        , archive-sig
        , archive-libarchive
        , archive-tar
        -- , archive-tar-bytestring

max-backjumps: 40000

allow-newer:
  lzma:base
  polyparse:base

constraints: archive-sig +development
           , archive-libarchive +development
           , archive-tar +development
           , archive-tar-bytestring +development
           , hstar +development
           , zlib +pkg-config
